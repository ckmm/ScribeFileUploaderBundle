{
	"name"        : "scribe/file-uploader-bundle",
	"license"     : "MIT",
	"type"        : "project",
	"homepage"    : "https://scr.be/file-uploader-bundle",
	"description" : "Bundle to help implement Blueimp's multi-file upload functionality in Symfony.",
	"support" : {
		"email"  : "rmf@scr.be",
		"issues" : "https://ScribeFileUploaderBundle.docs.scribe.tools/issues",
		"source" : "https://ScribeFileUploaderBundle.docs.scribe.tools/git",
		"irc"    : "irc://irc.efnet.org/scribe"
	},
	"keywords"    : [
		"file-uploader-bundle",
		"symfony-file-uploader-bundle",
		"scribe-file-uploader-bundle",
		"file-uploader",
		"uploader",
		"blueimp",
		"bundle",
		"symfony",
		"symfony2",
		"symfony-framework",
		"scribe",
		"scr",
		"scribenet",
		"scribeinc",
		"scribe-software",
		"scribe-services"
	],
	"authors": [
		{
			"name"     : "Rob Frawley 2nd",
			"email"    : "rmf@scr.be",
			"homepage" : "https://scr.be/rmf",
			"role"     : "Project Lead"
		},
		{
			"name"  : "Thomas Boutell",
			"email" : "tom@punkave.com"
		},
		{
			"name"  : "Wes John-Alder",
			"email" : "wes@punkave.com"
		}
	],
	"require" : {
		"php"                                  : ">=5.6.4",
		"scribe/mantle-bundle"                 : "2.0.*@dev",
		"scribe/cache-bundle"                  : "2.0.*@dev",
		"kriswallsmith/assetic"                : "1.3.*@dev",
		"mindscape/raygun4php"                 : "dev-master"
	},
	"require-dev" : {
		"ext-memcached"                        : "~2.2",
		"ext-igbinary"                         : "~1.2",
		"ext-tidy"                             : "~2.0",
		"symfony/phpunit-bridge"               : "2.7.*@dev",
		"symfony/symfony"                      : "2.7.*@dev",
		"phpunit/phpunit"                      : "~4.6.4",
		"sensio/generator-bundle"              : "3.0.*@dev",
		"doctrine/doctrine-fixtures-bundle"    : "2.2.*@dev",
		"sensiolabs/security-checker"          : "2.0.*@dev",
		"scrutinizer/ocular"                   : "dev-master",
		"satooshi/php-coveralls"               : "dev-multipleroot",
		"chriskite/phactory"                   : "~0.4.3",
		"sami/sami"                            : "*@dev",
		"pdepend/pdepend"                      : "2.0.6"
	},
	"autoload": {
		"psr-4": {
			"Scribe\\FileUploaderBundle\\": "src/"
		}
	},
	"config" : {
		"bin-dir"           : "bin",
		"preferred-install" : "auto",
		"minimum-stability" : "dev"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0-dev"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "https://github.com/scribenet/php-coveralls.git"
		}
	]
}
